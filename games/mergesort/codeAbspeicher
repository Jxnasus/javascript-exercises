function zahlenReiheBreakdown {
    let untereGeteilteH채lfte = zahlenReihe.slice(0,zahlenReihe.length/2)
    let obereGeteilteH채lfte = zahlenReihe.slice(-0, zahlenReihe.length/2)
    if (untereGeteilteH채lfte.length && obereGeteilteH채lfte.length == 1) {
        break;
    }
    else {
        zahlenReiheBreakdown()
    }
}



function insertionSort(array) {
    for (let i = 0; i <= array.length; i++) {
        for (let o = i; o >= 0; o--) {
            if (array[o] < array[o-1]) {
            let zahlenSpeicher = array[o]
            array[o] = array[o-1]
            array[o-1] = zahlenSpeicher
            }
        }
    }
    return array;
}
